      ],
      "flags": {
        "json": {
          "name": "json",
          "type": "boolean",
          "description": "Format output as json.",
          "helpGroup": "GLOBAL",
          "allowNo": false
        },
        "log-level": {
          "name": "log-level",
          "type": "option",
          "summary": "Specify level for logging.",
          "helpGroup": "GLOBAL",
          "multiple": false,
          "options": [
            "debug",
