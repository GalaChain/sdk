  "peerDependencies": {
    "bignumber.js": "*",
    "class-transformer": "*",
    "class-validator": "*",
    "class-validator-jsonschema": "*",
    "dd-trace": "*",
    "elliptic": "*",
    "json-stringify-deterministic": "*"
  },
  "devDependencies": {
    "@types/elliptic": "*",
    "@types/node": "^16.11.41",
    "@types/setimmediate": "^1.0.2",
    "dd-trace": "^4.20.0",
    "git-branch-is": "^4.0.0",
    "madge": "^5.0.1",
