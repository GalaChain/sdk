{
  "name": "@gala-chain/ui",
  "version": "0.0.0",
  "type": "module",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./gala-chain-ui.umd.js",
  "module": "./gala-chain-ui.es.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./gala-chain-ui.es.js",
      "require": "./gala-chain-ui.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build --config vite.config.lib.ts",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@gala-chain/api": "^1.2.0",
    "@gala-chain/connect": "^1.2.0",
    "@heroicons/vue": "^2.1.3",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "bignumber.js": "^9.1.2",
    "pinia": "^2.1.7",
    "primevue": "^3.52.0",
    "tailwindcss": "^3.4.4",
    "uuid": "^10.0.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@nx/vite": "^19.3.2",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  }
